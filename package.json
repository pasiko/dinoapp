{
  "name": "dino-warranty-app",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node index.js",
    "server_dev": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"cross-env NODE_ENV=development npm run client\" \"npm run server_dev\"",
    "start": "npm run server",
    "build": "rimraf ./build && cd client && npm run build && cd .. && copyfiles -u 1 ./client/build/** ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "copyfiles": "^2.0.0",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3"
  }
}
